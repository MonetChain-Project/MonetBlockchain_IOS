#import "UIView+tapGestureRec.h"

@implementation UIView (tapGestureRec)

- (void)addTapTarget:(nullable id)target action:(nullable SEL)action
{
    UIGestureRecognizer *tapRec = [[UITapGestureRecognizer alloc] initWithTarget:target action:action];
    [self addGestureRecognizer:tapRec];
}

@end
